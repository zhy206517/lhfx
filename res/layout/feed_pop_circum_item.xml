<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="44dip"
	android:paddingLeft="5dip" android:paddingRight="2dip"
	android:background="@drawable/feed_theme_collection_bg"
	android:gravity="center_vertical">
	<ImageView android:id="@+id/cur_loc_icon"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:layout_alignParentLeft="true" android:src="@drawable/cur_plcae_icon"
		android:layout_centerVertical="true" android:focusable="false"
		android:focusableInTouchMode="false" 
		android:visibility="gone"/>
	<TextView android:id="@+id/pop_text" style="@style/pop_item" android:layout_alignParentLeft="false"  android:layout_toRightOf="@+id/cur_loc_icon"/>
	<ImageView android:id="@+id/pop_circum_delete"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:layout_alignParentRight="true" android:src="@drawable/feed_pop_item_delete"
		android:layout_centerVertical="true" android:focusable="false"
		android:focusableInTouchMode="false" />
</RelativeLayout>	