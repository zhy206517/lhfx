<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
	<LinearLayout android:orientation="vertical"
	    android:layout_width="match_parent"
    	android:layout_height="wrap_content"
	    android:layout_marginTop="16dp"
	    android:layout_marginLeft="12dp"
		android:layout_marginRight="12dp"
		android:layout_marginBottom="16dp"
		android:layout_gravity="center_horizontal"
	    android:background="@drawable/login_edittext_bg" >
		<RelativeLayout android:id="@+id/login_sina"
				android:layout_width="fill_parent"
				android:layout_height="45dp"
				android:layout_marginLeft="12dp"
				android:orientation="horizontal">
				<ImageView android:id="@+id/logo_sina" 
				    android:layout_width="40dip"
				    android:layout_height="40dip"
				    android:src="@drawable/login_icon_sina"
				    android:scaleType="fitXY"
				    android:layout_gravity="center_vertical"
				    android:layout_alignParentLeft="true"/>
		  	    <TextView android:textSize="18sp"
		  	        android:layout_marginLeft="5dp"
		  	        android:layout_toRightOf="@+id/logo_sina"
		  	        android:layout_gravity="center_vertical"
	            	android:textColor="#333333"
					android:layout_width="220dp"
					android:layout_height="wrap_content"
					android:text="@string/login_account_sina"
					android:layout_centerVertical="true"/>
				<ImageView 
	  				android:layout_width="wrap_content"
	 			 	android:layout_height="wrap_content"
				  	android:src="@drawable/more_right_icon"
				  	android:layout_alignParentRight="true"
				  	android:layout_centerVertical="true"
				  	android:layout_marginRight="12dip"/>
		</RelativeLayout>
		<ImageView android:layout_height="wrap_content" 
					android:layout_width="fill_parent" 
					android:background="@drawable/feed_line" 
					android:scaleType="fitCenter"/>
		<RelativeLayout android:id="@+id/login_qq"
				android:layout_width="fill_parent"
				android:layout_height="45dp"
				android:layout_marginLeft="12dp"
				android:orientation="horizontal">
				<ImageView android:id="@+id/logo_qq" 
				    android:layout_width="40dip"
				    android:layout_height="40dip"
				    android:src="@drawable/login_icon_qq"
				    android:scaleType="fitXY"
				    android:layout_gravity="center_vertical"
				    android:layout_alignParentLeft="true" />
		  	    <TextView android:textSize="18sp"
		  	        android:layout_marginLeft="5dp"
		  	        android:layout_toRightOf="@+id/logo_qq"
		  	        android:layout_gravity="center_vertical"
	            	android:textColor="#333333"
					android:layout_width="220dp"
					android:layout_height="wrap_content"
					android:text="@string/login_account_qq"
					android:layout_centerVertical="true"/>
				<ImageView 
	  				android:layout_width="wrap_content"
	 			 	android:layout_height="wrap_content"
    			    android:scaleType="fitCenter"
				  	android:src="@drawable/more_right_icon"
				  	android:layout_alignParentRight="true"
				  	android:layout_centerVertical="true"
				  	android:layout_marginRight="12dip"/>
		</RelativeLayout>
		<ImageView android:layout_height="wrap_content" 
					android:layout_width="fill_parent" 
					android:background="@drawable/feed_line" 
					android:scaleType="fitCenter"/>
		<RelativeLayout android:id="@+id/login_renren"
				android:layout_width="fill_parent"
				android:layout_height="45dp"
				android:layout_marginLeft="12dp"
				android:orientation="horizontal">
				<ImageView android:id="@+id/logo_renren" 
				    android:layout_width="40dip"
				    android:layout_height="40dip"
				    android:src="@drawable/login_icon_renren"
				    android:scaleType="fitXY"
				    android:layout_gravity="center_vertical"
				    android:layout_alignParentLeft="true"/>
		  	    <TextView android:textSize="18sp"
		  	        android:layout_marginLeft="5dp"
		  	        android:layout_toRightOf="@+id/logo_renren"
		  	        android:layout_gravity="center_vertical"
	            	android:textColor="#333333"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:text="@string/login_account_renren"
					android:layout_centerVertical="true"/>
				<ImageView 
	  				android:layout_width="wrap_content"
	 			 	android:layout_height="wrap_content"
    			    android:scaleType="fitCenter"
				  	android:src="@drawable/more_right_icon"
				  	android:layout_alignParentRight="true"
				  	android:layout_centerVertical="true"
				  	android:layout_marginRight="12dip"/>
		</RelativeLayout>
		<ImageView android:layout_height="wrap_content" 
					android:layout_width="fill_parent" 
					android:background="@drawable/feed_line" 
					android:scaleType="fitCenter"/>
		<RelativeLayout android:id="@+id/login_baidu"
				android:layout_width="fill_parent"
				android:layout_height="45dp"
				android:layout_marginLeft="12dp"
				android:orientation="horizontal">
				<ImageView android:id="@+id/logo_baidu" 
				    android:layout_width="40dip"
				    android:layout_height="40dip"
				    android:src="@drawable/login_icon_baidu"
				    android:scaleType="fitXY"
				    android:layout_gravity="center_vertical"
				    android:layout_alignParentLeft="true"/>
		  	    <TextView android:textSize="18sp"
		  	        android:layout_marginLeft="5dp"
		  	        android:layout_toRightOf="@+id/logo_baidu"
		  	        android:layout_gravity="center_vertical"
	            	android:textColor="#333333"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:text="@string/login_account_baidu"
					android:layout_centerVertical="true"/>
				<ImageView 
	  				android:layout_width="wrap_content"
	 			 	android:layout_height="wrap_content"
    			    android:scaleType="fitCenter"
				  	android:src="@drawable/more_right_icon"
				  	android:layout_alignParentRight="true"
				  	android:layout_centerVertical="true"
				  	android:layout_marginRight="12dip"/>
		</RelativeLayout>
		<ImageView android:layout_height="wrap_content" 
					android:layout_width="fill_parent" 
					android:background="@drawable/feed_line" 
					android:scaleType="fitCenter"/>
		<RelativeLayout android:id="@+id/login_leho"
				android:layout_width="fill_parent"
				android:layout_height="45dp"
				android:layout_marginLeft="12dp"
				android:orientation="horizontal">
				<ImageView android:id="@+id/logo_leho" 
				    android:layout_width="40dip"
				    android:layout_height="40dip"
				    android:src="@drawable/login_icon_leho"
				    android:scaleType="fitXY"
				    android:layout_gravity="center_vertical"
				    android:layout_alignParentLeft="true"/>
		  	    <TextView android:textSize="18sp"
		  	        android:layout_marginLeft="5dp"
		  	        android:layout_toRightOf="@+id/logo_leho"
		  	        android:layout_gravity="center_vertical"
	            	android:textColor="#333333"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:text="@string/login_account_leho"
					android:layout_centerVertical="true"/>
				<ImageView 
	  				android:layout_width="wrap_content"
	 			 	android:layout_height="wrap_content"
    			    android:scaleType="fitCenter"
				  	android:src="@drawable/more_right_icon"
				  	android:layout_alignParentRight="true"
				  	android:layout_centerVertical="true"
				  	android:layout_marginRight="12dip"/>
		</RelativeLayout>
	</LinearLayout>
</LinearLayout>
